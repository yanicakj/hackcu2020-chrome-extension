{
  "manifest_version": 2,
  "name": "HackCU Study Helper",
  "description": "This extension will help you pass classes",
  "version": "0.1",
  "content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js","popup.js"]
    }
  ],
  "permissions": ["storage","activeTab","tabs"],
  "browser_action": {
    "default_icon": "icon3.png",
    "default_popup": "popup.html"
  }
}


